{{<layout}}
{{$content}}

    <h1>An error occurred</h1>
    <p>{{message}}</p>

    <h3>Exception information:</h3>
    <p><b>Message:</b> {{exceptionmessage}}</p>

    <h3>Stack trace:</h3>
    <pre>{{stacktrace}}</pre>

    <h3>Request Parameters:</h3>
    <pre>{{requestparams}}</pre>

{{/content}}
{{/layout}}